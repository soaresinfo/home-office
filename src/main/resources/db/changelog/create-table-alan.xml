<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<changeSet id="4" author="alan.franco">
		<createTable tableName="tb_alan">

			<column name="id" type="int" autoIncrement="true">
				<constraints primaryKey="true" nullable="false" />
			</column>

			<column name="numeroConfsite" type="varchar(50)" />

			<column name="nomePacote" type="varchar(50)" />

			<column name="url" type="varchar(50)" />

			<column name="status" type="varchar(50)" />

			<column name="dtCriacao" type="TIMESTAMP" />

			<column name="cnpj" type="varchar(50)" />

			<column name="registrationDate" type="TIMESTAMP" />

			<column name="isNew" type="BIT(1)" />

			<column name="price" type="numeric(18,3)" />
		</createTable>
	</changeSet>
</databaseChangeLog>